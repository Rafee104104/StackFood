{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^7.3 || ^8.0",
        "ext-curl": "*",
        "ext-json": "*",
        "brian2694/laravel-toastr": "^5.54",
        "doctrine/dbal": "^2.13",
        "fideloper/proxy": "^4.4",
        "laravel/cors": "^2.0",
        "gregwar/captcha": "^1.1",
        "grimzy/laravel-mysql-spatial": "5.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "intervention/image": "^2.5",
        "kingflamez/laravelrave": "^4.2",
        "laravel/framework": "^8.12",
        "laravel/passport": "^10.1",
        "laravel/tinker": "^2.5",
        "laravelpkg/laravelchk": "dev-master",
        "madnest/madzipper": "*",
        "mercadopago/dx-php": "2.4.3",
        "vonage/laravel": "^5.0",
        "paypal/checkout-sdk-php": "^1.0",
        "rap2hpoutre/fast-excel": "^3.0",
        "razorpay/razorpay": "^2.5",
        "stripe/stripe-php": "^7.77",
        "twilio/sdk": "^6.28",
        "unicodeveloper/laravel-paystack": "1.0.*"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.5",
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3.3"
    },
    "autoload": {
        "files": [
            "app/CentralLogics/banner.php",
            "app/CentralLogics/category.php",
            "app/CentralLogics/campaign.php",
            "app/CentralLogics/item.php",
            "app/CentralLogics/store.php",
            "app/CentralLogics/helpers.php",
            "app/helpers.php",
            "app/CentralLogics/order.php",
            "app/CentralLogics/coupon.php",
            "app/CentralLogics/sms_module.php",
            "app/CentralLogics/filemanager.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
            
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
